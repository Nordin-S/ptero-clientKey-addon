{
    "name": "Nordin-S/ptero-clientKey-addon",
    "description": "An addon for Pterodactyl Panel to manage user API keys via the application API.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nordin-S"
        }
    ],
    "require": {
        "php": "^8.1",
        "pterodactyl/panel": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Pterodactyl\\Addons\\UserApiKeys\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pterodactyl\\Addons\\UserApiKeys\\AddonServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
